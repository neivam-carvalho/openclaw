# Exemplo de arquivo .env.local
# Copie este arquivo para .env.local e preencha com suas credenciais

# Gateway Setup Password
SETUP_PASSWORD=seu-setup-password-aqui

# Gateway Authentication Token  
OPENCLAW_GATEWAY_TOKEN=seu-gateway-token-aqui

# Telegram Bot Token
TELEGRAM_BOT_TOKEN=seu-telegram-token-aqui

# Claude AI (opcional)
# CLAUDE_AI_SESSION_KEY=sua-chave-de-sessao-aqui

# Node Environment
NODE_ENV=production

# Porta (para desenvolvimento local)
PORT=8080

# Para Railway, use /data como diret√≥rio base
OPENCLAW_STATE_DIR=/data
OPENCLAW_WORKSPACE_DIR=/data/workspace
